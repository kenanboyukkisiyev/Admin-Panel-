@using Calena.Models;
@{
    ViewBag.Title = "Index";
}

<section class="page-title" style="background-image:url(images/background/2.jpg)">
    <div class="auto-container">
        <h1>Who We Are</h1>
        <ul class="bread-crumb clearfix">
            <li><a href="index.html">Home</a></li>
            <li class="active">About</li>
        </ul>
    </div>
</section>
<!--End Page Title-->
<!--Quality Section-->
<section class="quality-section">
    <div class="auto-container">
        <div class="row clearfix">
            <!--Title Column-->
            <div class="title-column col-md-6 col-sm-12 col-xs-12">
                <div class="inner-column">
                    <div class="sec-title">
                        <h2>Calena Provides You Quality <br><span class="purple-border">Global Solutions</span></h2>
                    </div>
                    <div class="text">Adipisicing consectetur elitsd eiusmod tempor sed cidolore magna aliqua  niad min veniam quis nosar mag exercitation ullamco laboris aliquip  comdo rue sede consequat ipsum duis aute dolor. Lorem ipsum sed dolor sit amets consectetur adipisicing elit sed dolor eiusmod tempor incididunt ut labore et dolore magna aliqua uat enim ad minim veniam quis nostrud.</div>
                </div>
            </div>
            <!--Skill Column-->
            <div class="skill-column col-md-6 col-sm-12 col-xs-12">

                <!--Skills-->
                <div class="skills">
                    <!--Skill Item-->
                    <div class="skill-item">
                        <div class="skill-header clearfix">
                            <div class="skill-title">web development</div>
                            <div class="skill-percentage"><div class="count-box"><span class="count-text" data-speed="2000" data-stop="70">0</span>%</div></div>
                        </div>
                        <div class="skill-bar">
                            <div class="bar-inner"><div class="bar progress-line" data-width="70"></div></div>
                        </div>
                    </div>
                    <!--Skill Item-->
                    <div class="skill-item">
                        <div class="skill-header clearfix">
                            <div class="skill-title">photography</div>
                            <div class="skill-percentage"><div class="count-box"><span class="count-text" data-speed="2000" data-stop="55">0</span>%</div></div>
                        </div>
                        <div class="skill-bar">
                            <div class="bar-inner"><div class="bar progress-line" data-width="55"></div></div>
                        </div>
                    </div>
                    <!--Skill Item-->
                    <div class="skill-item">
                        <div class="skill-header clearfix">
                            <div class="skill-title">marketing</div>
                            <div class="skill-percentage"><div class="count-box"><span class="count-text" data-speed="2000" data-stop="65">0</span>%</div></div>
                        </div>
                        <div class="skill-bar">
                            <div class="bar-inner"><div class="bar progress-line" data-width="65"></div></div>
                        </div>
                    </div>
                    <!--Skill Item-->
                    <div class="skill-item">
                        <div class="skill-header clearfix">
                            <div class="skill-title">search ranking</div>
                            <div class="skill-percentage"><div class="count-box"><span class="count-text" data-speed="2000" data-stop="80">0</span>%</div></div>
                        </div>
                        <div class="skill-bar">
                            <div class="bar-inner"><div class="bar progress-line" data-width="80"></div></div>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
</section>
<!--End Quality Section-->
<!--Team Section-->
<section class="team-section grey-bg">
    <div class="auto-container">
        <!--Sec Title-->
        <div class="sec-title centered">
            <h2>Team <span>Member</span></h2>
        </div>
        <!--End Sec Title-->
        <div class="row clearfix">
            <!--Team Member-->

            @foreach (Team item in ViewBag.Teams)
            {
                <div class="team-member col-md-3 col-sm-6 col-xs-12">
                    <div class="inner-box">
                        <div class="image">
                            <img src="~/Updates/@item.Photo" alt="" />
                        </div>
                        <div class="lower-box">
                            <h3><a href="#">@item.FullName</a></h3>
                            <div class="designation">@item.Position</div>
                        </div>
                    </div>
                </div>
            }




        </div>

    </div>
</section>
<!--End Team Section-->
<!--Mission Section-->
<section class="mission-section">
    <div class="auto-container">
        <div class="row clearfix">
            <div class="image-column col-md-6 col-sm-12 col-xs-12">
                <div class="inner-column">
                    <div class="image">
                        <img src="~/Updates/@ViewBag.about.Photo" alt="" />
                    </div>
                </div>
            </div>
            <!--Content Column-->
            <div class="content-column col-md-6 col-sm-12 col-xs-12">
                <div class="inner-column">
                    <!--Sec Title-->
                    <div class="sec-title">
                        <h2>Our Mission is to Make You <br><span class="purple-border">Grow Higher</span></h2>
                    </div>
                    <!--Mission Info Tabs-->
                    <div class="mission-info-tabs">
                        <!--Product Tabs-->
                        <div class="prod-tabs tabs-box">

                            <!--Tab Btns-->
                            <ul class="tab-btns tab-buttons clearfix">
                                <li data-tab="#prod-vision" class="tab-btn active-btn">@ViewBag.about.Vision</li>
                                <li data-tab="#prod-history" class="tab-btn">@ViewBag.about.History</li>
                            </ul>

                            <!--Tabs Container-->
                            <div class="tabs-content">

                                <!--Tab / Active Tab-->
                                <div class="tab active-tab" id="prod-vision">
                                    <div class="content">
                                        <div class="text">
                                            @ViewBag.about.Desc 
                                        </div>
                                        <!--End Business Info Tabs-->
                                        <div class="signature"><img src="~/Updates/@ViewBag.about.Signature" alt="" /></div>
                                    </div>
                                </div>

                                <!--Tab-->
                                <div class="tab" id="prod-history">
                                    <div class="content">
                                        <div class="text">
                                           @ViewBag.about.Desc
                                        </div>
                                    </div>
                                </div>
                                <!--End Tab-->

                            </div>

                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!--Counter Section-->
        <section class="counter-section">
            <div class="fact-counter">
                <div class="row clearfix">
                    @foreach (AboutService item in ViewBag.aboutservice)
                    {
                        <div class="column counter-column col-md-3 col-sm-6 col-xs-12">
                            <div class="inner">
                                <div class="count-outer count-box">
                                    <span class="count-text" data-speed="3000" data-stop="@item.Value">0</span>
                                </div>
                                <h4 class="counter-title">@item.Name</h4>
                            </div>
                        </div>
                    }
                   

                    <
                   

                </div>
            </div>
        </section>
        <!--End Counter Section-->

    </div>
</section>
<!--End Mission Section-->
<!--Subscribe Section-->
<section class="subscribe-section grey-bg">
    <div class="auto-container">
        <div class="inner-section">
            <div class="row clearfix">
                <div class="title-column col-md-4 col-sm-12 col-xs-12">
                    <h2>Subscribe to Newsletter</h2>
                </div>
                <div class="form-column col-md-8 col-sm-12 col-xs-12">

                    <!--Subscribe Section-->
                    <div class="subscribe-form">
                        <form method="post" action="http://t.commonsupport.com/calena/contact.html">
                            <div class="row clearfix">
                                <!--Column-->
                                <div class="column col-md-9 col-sm-12 col-xs-12">
                                    <div class="row clearfix">
                                        <div class="form-group col-md-6 col-sm-6 col-xs-12">
                                            <input type="text" name="name" value="" placeholder="Your Name" required>
                                        </div>
                                        <div class="form-group col-md-6 col-sm-6 col-xs-12">
                                            <input type="email" name="email" value="" placeholder="Email address" required>
                                        </div>
                                    </div>
                                </div>
                                <!--Column-->
                                <div class="column col-md-3 col-sm-12 col-xs-12">
                                    <div class="form-group">
                                        <button type="submit" class="theme-btn btn-style-one">subscribe</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>
    </div>
</section>
<!--End Subscribe Section-->

