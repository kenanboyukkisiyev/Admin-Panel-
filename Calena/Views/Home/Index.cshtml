                    <div class="mission-info-tabs">
                        <!--Product Tabs-->
                        <div class="prod-tabs tabs-box">

                            <!--Tab Btns-->
                            <ul class="tab-btns tab-buttons clearfix">
                                <li data-tab="#prod-vision" class="tab-btn active-btn">@ViewBag.about.Vision</li>
                                <li data-tab="#prod-history" class="tab-btn">@ViewBag.about.History</li>
                            </ul>

                            <!--Tabs Container-->
                            <div class="tabs-content">

                                <!--Tab / Active Tab-->
                                <div class="tab active-tab" id="prod-vision">
                                    <div class="content">
                                        <div class="text">
                                           @ViewBag.About.Desc
                                        </div>
                                        <!--End Business Info Tabs-->
                                        <div class="signature"><img src="~/Updates/@ViewBag.About.Signature" alt="" /></div>
                                    </div>
                                </div>

                                <!--Tab-->
                                <div class="tab" id="prod-history">
                                    <div class="content">
                                        <div class="text">
                                            @ViewBag.About.Desc
                                        </div>
                                    </div>
                                </div>
                                <!--End Tab-->

                            </div>

                        </div>

                    </div>
                </div>
            </div>
        </div>

        <!--Counter Section-->
        <section class="counter-section">
            <div class="fact-counter">
                <div class="row clearfix">

                    @foreach (AboutService item in ViewBag.aboutservice)
                    {
                        <div class="column counter-column col-md-3 col-sm-4 col-xs-12">
                            <div class="inner">
                                <div class="count-outer count-box">
                                    <span class="count-text" data-speed="3000" data-stop="@item.Value">0</span>
                                </div>
                                <h4 class="counter-title">@item.Name</h4>
                            </div>
                        </div>
                    }
                   

                </div>
            </div>
        </section>
        <!--End Counter Section-->

    </div>
</section>
